<h1><%= title %></h1>
<% //let errors = "ERROR TEST" %>
<form action="" class="login">

    <div>
        <label for="account_email">Email<abbr class="req" title="required"> *</abbr></label>
        <input name="account_email" id="account_email" type="email" required placeholder="Enter your email">
    </div>
    
    <div>
        <label for="account_password">Password<abbr class="req" title="required"> *</abbr></label>
        <input name="account_password" id="account_password" type="password" required placeholder="Enter your password">

    </div>

    <%- messages() %>

    <input type="submit" value="Sign In">

    <p>No account? <a href="/account/register">Sign up!</a></p>
</form>